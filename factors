#!/usr/bin/env python3

import sys

def main(file_path):
    with open(file_path, 'r') as file:
        for line in file:
            number = int(line.strip())
            factor1, factor2 = factorize(number)
            print(f"{number}={factor1}*{factor2}")

def factorize(n):
    if n % 2 == 0:
        return n // 2, 2
    factor = 3
    while factor * factor <= n:
        if n % factor == 0:
            return n // factor, factor
        factor += 2
    return 1, n

if __name__ == '__main__':
    if len(sys.argv) != 2:
        print('Usage: ./factors <file>')
        sys.exit(1)
    main(sys.argv[1])
