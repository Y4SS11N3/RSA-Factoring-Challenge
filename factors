#!/usr/bin/env python3

import sys
from subprocess import Popen, PIPE

def main(file_path):
    with open(file_path, 'r') as file:
        for line in file:
            number = int(line.strip())
            factor1, factor2 = factorize_first_pair(number)
            print(f"{number}={factor1}*{factor2}")

def factorize_first_pair(n):
    process = Popen(['factor', str(n)], stdout=PIPE)
    (output, _) = process.communicate()
    output = output.decode('utf-8').strip()
    
    _, factors_str = output.split(': ')
    factors = factors_str.split(' ')
    return int(factors[0]), n // int(factors[0])

def format_output(number, factor1, factor2):
    if factor1 > factor2:
        return f"{number}={factor1}*{factor2}"
    else:
        return f"{number}={factor2}*{factor1}"

if __name__ == '__main__':
    if len(sys.argv) != 2:
        print('Usage: ./factors <file>')
        sys.exit(1)
    main(sys.argv[1])
